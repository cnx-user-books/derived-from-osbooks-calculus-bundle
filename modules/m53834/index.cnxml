<colspec colnum="7" colname="c7"/>
<thead>
<row valign="top">
<entry valign="top" align="center"><emphasis effect="italics">t</emphasis></entry>
<entry valign="top" align="center"><m:math><m:mrow><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math></entry>
<entry valign="top" align="center"><m:math><m:mrow><m:mi>y</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math></entry>
<entry valign="top" align="center"/>
<entry valign="top" align="center"><emphasis effect="italics">t</emphasis></entry>
<entry valign="top" align="center"><m:math><m:mrow><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math></entry>
<entry valign="top" align="center"><m:math><m:mrow><m:mi>y</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math></entry>
